    "@types/googlemaps": "3.39.6",
    "@types/markerclustererplus": "2.1.33",
    "ajv": "6.12.2",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.21.7",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.7",
    "@testing-library/user-event": "^12.1.0",
    "@types/history": "^4.7.7",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
