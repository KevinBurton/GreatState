{
  "name": "greatstate-material",
  "version": "1.9.0",
  "description": "Great State web page coded by Kevin Burton (rkevinburton@charter.net)",
  "private": false,
  "main": "dist/index.js",
  "dependencies": {
    "@material-ui/core": "4.10.0",
    "@material-ui/icons": "4.9.1",
    "@react-google-maps/api": "^1.9.9",
    "axios": "^0.19.2",
    "chartist": "0.10.1",
    "classnames": "2.2.6",
    "dotenv": "^8.2.0",
